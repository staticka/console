{
    "name": "staticka/console",
    "description": "Terminal-based static generator for Staticka.",
    "keywords": ["console", "generator", "static site", "staticka"],
    "homepage": "https://staticka.github.io/console/",
    "license": "MIT",
    "authors":
    [
        {
            "email": "rougingutib@gmail.com",
            "homepage": "https://roug.in/",
            "name": "Rougin Gutib",
            "role": "Software Engineer"
        }
    ],
    "require":
    {
        "php": ">=5.4.0",
        "scssphp/scssphp": "~1.1",
        "staticka/staticka": "dev-master",
        "symfony/console": ">=2.8.0"
    },
    "require-dev":
    {
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1.0"
    },
    "bin":
    [
        "bin/staticka",
        "bin/staticka.php"
    ],
    "autoload":
    {
        "psr-4":
        {
            "Staticka\\Console\\": "src"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Staticka\\Console\\": "tests"
        }
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.0-dev"
        }
    }
}